parameters:
	level: 6
	paths:
		- src
		- tests
	ignoreErrors:
		- '#Property [a-zA-Z0-9\\_]+::\$[a-zA-Z0-9\\_]+ has no typehint specified.#'
		- '#Method [a-zA-Z0-9\\_]+::[a-zA-Z0-9\\_\(\)]+ return type with generic interface [a-zA-Z0-9\\_]+ does not specify its types#'
		- '#Method [a-zA-Z0-9\\_]+::[a-zA-Z0-9\\_\(\)]+ has parameter \$[a-zA-Z0-9\\_]+ with no value type specified in iterable type array#'
		- '#return type has no value type specified in iterable type array#'